<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ManHinh3">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="412dp"
        android:layout_height="205dp"
        android:src="@drawable/giaodienchung"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageTiepTheo"
        android:layout_width="156dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/nut_tieptheo"
        app:layout_constraintEnd_toStartOf="@+id/imageHome"
        app:layout_constraintHorizontal_bias="0.803"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"></ImageView>

    <ImageView
        android:id="@+id/imageHome"
        android:layout_width="69dp"
        android:layout_height="66dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/nut_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:ignore="MissingConstraints"></ImageView>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="366dp"
        android:layout_height="93dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/chao3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageTiepTheo"></ImageView>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="344dp"
        android:layout_height="238dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/nenchungmau23"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="MissingConstraints"></ImageView>

    <ImageView
        android:id="@+id/image2chunhat"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/hinhvuong"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        tools:ignore="MissingConstraints"></ImageView>

    <ImageView
        android:id="@+id/image2tron"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/hinhtron"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toEndOf="@+id/image2chunhat"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        tools:ignore="MissingConstraints"></ImageView>

    <ImageView
        android:id="@+id/image2ngoisao"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ngoisao"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/image2chunhat"
        tools:ignore="MissingConstraints"></ImageView>

    <ImageView
        android:id="@+id/image2tamgiac"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="12dp"
        android:src="@drawable/tamgiac"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/image2ngoisao"
        app:layout_constraintTop_toBottomOf="@+id/image2tron"
        tools:ignore="MissingConstraints"></ImageView>
</androidx.constraintlayout.widget.ConstraintLayout>